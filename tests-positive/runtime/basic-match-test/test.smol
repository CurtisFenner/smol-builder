package test;

import core;

union Un {
	var a Int;
	var b Int;

	static makeA(n Int) Un {
		return new(a = n);
	}

	static makeB(n Int) Un {
		return new(b = n);
	}
}

class Test {
	static main!() Unit {
		match Un.makeA(7) {
			case n a {
				do core:Out.println!("a" ++ core:ASCII.formatInt(n));
			}
			case n b {
				do core:Out.println!("b" ++ core:ASCII.formatInt(n));
			}
		}

		match Un.makeB(9) {
			case n a {
				do core:Out.println!("a" ++ core:ASCII.formatInt(n));
			}
			case n b {
				do core:Out.println!("b" ++ core:ASCII.formatInt(n));
			}
		}
	}
}
