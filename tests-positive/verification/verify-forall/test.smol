class Con {
	method length() Int
	ensures (return < 0).not() {
		return 3;
	}


	method valid(index Int) Boolean {
		return ((index == 0).or(0 < index)).and(index < 3);
	}

	method get(index Int) String
	requires this.valid(index) {
		if index == 0 {
			return "alpha";
		} elseif index == 1 {
			return "beta";
		}
		return "gamma";
	}
}

