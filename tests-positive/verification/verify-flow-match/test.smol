package test;

union U {
	var alpha Unit;
	var beta Unit;

	method foo() Unit {
		match this {
			case alpha alpha {
				assert this is alpha;
			}
			case beta beta {
				assert this is beta;
				return beta;
			}
		}
		assert (this is beta).not();
		assert this is alpha;
		return unit;
	}
}

class Test {
	static main() Unit {

	}
}
